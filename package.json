{
  "name": "dogecoin-vs-code",
  "displayName": "Dogecoin for VS  Code",
  "description": "Check Dogecoin price easily and see how it goes to the moon!",
  "version": "0.0.1",
  "publisher": "felipecespedes",
  "author": {
    "name": "Felipe CÃ©spedes",
    "url": "https://felipecespedes.github.io"
  },
  "engines": {
    "vscode": "^1.53.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:dogecoin-vs-code.sidebarView",
    "onCommand:dogecoin-vs-code.checkPrice",
    "onCommand:dogecoin-vs-code.start"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "dogecoin-explorer",
          "title": "Dogecoin",
          "icon": "resources/img/dogecoin.svg"
        }
      ]
    },
    "views": {
      "dogecoin-explorer": [
        {
          "type": "webview",
          "id": "dogecoin-vs-code.sidebarView",
          "name": "Dogecoin",
          "icon": "resources/img/dogecoin.svg",
          "contextualTitle": "Dogecoin"
        }
      ]
    },
    "commands": [
      {
        "command": "dogecoin-vs-code.checkPrice",
        "title": "Check Price",
        "icon": "resources/img/icon.png",
        "category": "Dogecoin"
      },
      {
        "command": "dogecoin-vs-code.start",
        "title": "Check Dogecoin",
        "icon": "resources/img/icon.png",
        "category": "Dogecoin"
      }
    ],
    "keybindings": [
      {
        "command": "dogecoin-vs-code.checkPrice",
        "key": "ctrl+alt+p",
        "mac": "ctrl+alt+p"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./ && rollup -c",
    "compile-svelte": "rollup -c",
    "watch": "tsc -watch -p ./",
    "watch-svelte": "rollup -c -w",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.1.1",
    "@tsconfig/svelte": "^1.0.10",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.53.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.1.33",
    "svelte-preprocess": "^4.6.6",
    "ts-loader": "^8.0.16",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@types/bignumber.js": "^5.0.0",
    "@types/chart.js": "^2.9.30",
    "@types/lodash": "^4.14.168",
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.1",
    "chart.js": "^2.9.4",
    "lodash": "^4.17.20",
    "svelte-fa": "^2.2.0"
  }
}
