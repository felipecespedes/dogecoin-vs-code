{
  "name": "dogecoin-vs-code",
  "displayName": "Dogecoin for VS  Code",
  "description": "Check Dogecoin price easily and see how it goes to the moon!",
  "version": "0.0.1",
  "publisher": "felipecespedes",
  "author": {
    "name": "Felipe CÃ©spedes",
    "url": "https://felipecespedes.github.io"
  },
  "engines": {
    "vscode": "^1.53.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:dogecoin-vs-code-sidebar",
    "onCommand:dogecoin-vs-code.checkPrice"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "dogecoin-vs-code-sidebar-view",
          "title": "Dogecoin",
          "icon": "media/dogecoin.svg"
        }
      ]
    },
    "views": {
      "dogecoin-vs-code-sidebar-view": [
        {
          "type": "webview",
          "id": "dogecoin-vs-code-sidebar",
          "name": "Dogecoin",
          "icon": "media/dogecoin.svg",
          "contextualTitle": "Dogecoin"
        }
      ]
    },
    "commands": [
      {
        "command": "dogecoin-vs-code.checkPrice",
        "title": "Dogecoin: Check Price",
        "icon": "resources/img/icon.png"
      }
    ],
    "keybindings": [
      {
          "command": "dogecoin-vs-code.checkPrice",
          "key": "ctrl+alt+p",
          "mac": "ctrl+alt+p"
      }
  ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.53.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "axios": "^0.21.1"
  }
}
